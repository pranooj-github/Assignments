package com.springcore.assignment.springassignments.second;

import java.util.Set;

public class Question {
 int qustion_id;
 String question;
 Set<String> answers;
public Question(int qustion_id, String question, Set<String> answers) {
	super();
	this.qustion_id = qustion_id;
	this.question = question;
	this.answers = answers;
}
 public void showAnswers()
 {
	 System.out.println(qustion_id+ " Q: "+question);
	 System.out.println("Answers = "+answers);
 }
@Override
public String toString() {
	return "Question [qustion_id=" + qustion_id + ", question=" + question + ", answers=" + answers + "]";
}
 
 
}
